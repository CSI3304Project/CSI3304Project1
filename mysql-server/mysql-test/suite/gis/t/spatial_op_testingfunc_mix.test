SELECT AsText(ST_Union(ST_Intersection(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star)));
SELECT ST_Equals(GeomFromText(@star),ST_Union(ST_Intersection(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star)));
SELECT ST_Equals(g,ST_Union(ST_Intersection(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star))) FROM gis_geometrycollection WHERE fid=100;
