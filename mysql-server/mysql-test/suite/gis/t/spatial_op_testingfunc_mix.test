SELECT AsText(GeomFromText(@star));
SELECT AsText(GeomFromText(@star_elem_vertical));
SELECT AsText(ST_Difference(GeomFromText(@star),GeomFromText(@star_elem_vertical)));
SELECT AsText(ST_Union(ST_Difference(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star)));
SELECT ST_Equals(GeomFromText(@star),ST_Union(ST_Difference(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star)));
SELECT ST_Equals(g,ST_Union(ST_Difference(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star))) FROM gis_geometrycollection WHERE fid=100;
SELECT ST_Disjoint(GeomFromText(@star),ST_Union(ST_Difference(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star)));
SELECT ST_Disjoint(g,ST_Union(ST_Difference(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star))) FROM gis_geometrycollection WHERE fid=100;

SELECT AsText(ST_Symdifference(GeomFromText(@star),GeomFromText(@star_elem_vertical)));
SELECT AsText(ST_Union(ST_Symdifference(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star)));
SELECT ST_Equals(GeomFromText(@star),ST_Union(ST_Symdifference(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star)));
SELECT ST_Equals(g,ST_Union(ST_Symdifference(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star))) FROM gis_geometrycollection WHERE fid=100;
SELECT ST_Intersects(GeomFromText(@star),ST_Union(ST_Symdifference(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star)));
SELECT ST_Intersects(g,ST_Union(ST_Symdifference(GeomFromText(@star),GeomFromText(@star_elem_vertical)),GeomFromtext(@star))) FROM gis_geometrycollection WHERE fid=100;

